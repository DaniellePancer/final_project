#include <stdio.h>


int my_scanf(const char *format, ...);

int main(void) {
    int d, x, b;
    float f1, f2;
    char s[50];
    char c1, c2;
    unsigned u;
    char q1[50], q2[50];


    printf("Input: 77 1F 1101 6.28 -12.34 hello K 1024 \"first quote\" \"second quote\"\n\n");

    int assigned = my_scanf("%d %x %b %f %f %s %c %u %q %q",
                            &d, &x, &b, &f1, &f2, s, &c1, &u, q1, q2);

    printf("Assigned: %d\n", assigned);
    printf("d = %d\n", d);
    printf("x = %d\n", x);
    printf("b = %d\n", b);
    printf("f1 = %.2f\n", f1);
    printf("f2 = %.2f\n", f2);
    printf("s = %s\n", s);
    printf("c1 = %c\n", c1);
    printf("u = %u\n", u);
    printf("q1 = %s\n", q1);
    printf("q2 = %s\n", q2);

    return 0;
}
